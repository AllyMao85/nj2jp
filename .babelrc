{
  "presets": [
    "es2016",
    "es2015",
    "react",
    "stage-2"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        "transform-react-jsx-source",
        "transform-react-display-name",
        "transform-export-extensions"
      ],
    },
    "production": {
      "presets": [
        ["es2015", { "modules": false }],
        "react",
        "stage-2"
      ],
      "plugins": [],
    }
  }
}
{ "presets":
  [[ "env",
    { "targets":
      { "node": true },
      "loose": true,
      "includes": ["babel-plugin-transform-object-rest-spread"]
    }
  ],
  "stage-0"]
}
