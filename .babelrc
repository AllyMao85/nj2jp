  "presets": ["es2015", "react", "stage-2"],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        "transform-react-jsx-source",
        "transform-react-display-name",
        "transform-export-extensions"
      ],
    },
    "production": {
      "plugins": [
        "transform-react-jsx-source",
        "transform-react-display-name",
        "transform-export-extensions"
      ],
    }
  }
}
